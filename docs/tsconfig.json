{
  "compilerOptions": {
    "strict": false,
    "target": "es5",
    "jsx": "react-jsx",
    "module": "CommonJS",
    "esModuleInterop": true,
    "removeComments": false,
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "lib": ["ES6", "DOM", "ES2017"],
    "declaration": true,
    "resolveJsonModule": true,
    "baseUrl": "."
  }
}
