# editorconfig.org

root = true

[*]
indent_style = tab
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false
indent_style = space
indent_size = 2

[.nvmrc]
insert_final_newline = false

[{*.json,*.yml}]
indent_style = space
indent_size = 2
